//Teste06.c

#include <stdio.h>;
#include <stdlib.h>;

int main(int argc, char *argv[]){
    float altura = 0.0, pesoIdeal = 0.0;


    printf("Informe uma altura : :\n");
    scanf("%f", &altura);

    pesoIdeal = (72.7*altura) – 58;

    printf("O peso ideal desta pessoa é : %f00.00\n", pesoIdeal);
}
